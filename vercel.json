
{
  "version": 2,
  "name": "node-js",
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "regions": ["sfo1"],
  "cache": {
    "headers": {
      "/*": {
        "cache-control": "max-age=0, must-revalidate"
      }
    }
  }
}
